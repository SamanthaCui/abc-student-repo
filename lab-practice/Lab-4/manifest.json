{
  "name":"Replacer",
  "version":"1.0",
  "description": "My first extention which will replace me",
  "manifest_version": 2,
  "permission": [
    "activeTab"
  ],
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["contentscript.js"]
    }
  ],
  "browser_action":{
    "default_icon":"tent.png",
    "default_popup":"popup/index.html"
  }
}
